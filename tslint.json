{
  "defaultSeverity": "error",
  "extends": [
    "tslint:all"
  ],
  "jsRules": {},
  "rules": {
    "semicolon": [true, "never"],
    "quotemark": [true, "single"],
    "array-type": [true, "generic"],

    "completed-docs": false,
    "newline-per-chained-call": false,
    "file-name-casing": [true, "ignored"],
    "member-ordering": false,
    "jsdoc-format": false,
    "no-null-keyword": false,
    "variable-name": [true, "allow-leading-underscore", "allow-pascal-case"],
    "max-classes-per-file": false,
    "binary-expression-operand-order": false,
    "prefer-function-over-method": false,
    "object-literal-key-quotes": [true, "consistent"],
    "only-arrow-functions": false,
    "comment-format": [true, "check-space"],
    "object-literal-sort-keys": false,
    "prefer-for-of": false,
    "no-unnecessary-class": [true, "allow-static-only"],
    "no-empty": false,
    "interface-name": false,
    "match-default-export-name": false,
    "no-unbound-method": false,
    "no-floating-promises": false,
    "no-void-expression": false,
    "no-irregular-whitespace": false,
    "strict-comparisons": [
      true,
      {
        "allow-object-equal-comparison": false
      }
    ],

    "no-default-export": false,
    "no-default-import": false,
    "prefer-conditional-expression": false,

    "no-magic-numbers": false,
    "switch-default": false,
    "switch-final-break": [true, "always"],

    "typedef": false
  },
  "rulesDirectory": []
}
